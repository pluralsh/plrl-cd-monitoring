prometheus:
  prometheusSpec:
    remoteWrite:
    - url: https://prometheus.boot-aws.onplural.sh/api/v1/write
      name: mgmt-cluster-prometheus
      basic_auth:
        username: 
          name: basic-auth-remote-prom
          key: user
        password: 
          name: basic-auth-remote-prom
          key: password
      write_relabel_configs:
        - source_labels: []  
          target_label: 'cluster'
          replacement: {{ cluster.Handle }}

